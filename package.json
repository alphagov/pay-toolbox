{
  "name": "pay-toolbox",
  "version": "0.3.0",
  "description": "Internal administrative tools service for the GOV.UK Pay products.",
  "engines": {
    "node": "^12.22.7",
    "npm": "^7.5.4"
  },
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "tsc-watch --noClear --onSuccess \"./scripts/run-dev\"",
    "lint": "eslint \"src/**/*.{js,ts,tsx}\"",
    "test": "npm run test:unit",
    "test:unit": "AUTH_GITHUB_ENABLED=true mocha -r ts-node/register -r dotenv/config --recursive \"./src/**/*.spec.{js,ts}\" dotenv_config_path=src/tests/.env --exit",
    "compile": "npm run build",
    "build": "scripts/build && npm run build:typescript && npm run build-browser && npm run build:copy && npm run build:sass",
    "build-browser": "webpack --mode production",
    "build-browser-dev": "webpack -w --mode development",
    "build:typescript": "tsc",
    "build:sass": "scripts/compile-assets",
    "build:copy": "copyfiles --up 1 src/**/*.njk dist/",
    "preinstall": "echo \"Preinstall hook\" && exit 0",
    "prepare": "npm run build:sass && npm run build-browser",
    "postinstall": "echo \"postinstall set to run with prepare (only in dev)\" && exit 0",
    "version": "echo $npm_package_version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alphagov/pay-toolbox.git"
  },
  "license": "MIT",
  "homepage": "https://github.com/alphagov/pay-toolbox#readme",
  "dependencies": {
    "@aws-sdk/client-ecs": "~3.17.0",
    "@aws-sdk/client-s3": "~3.17.0",
    "@govuk-pay/pay-js-commons": "3.3.5",
    "@sentry/node": "^6.12.0",
    "axios": "0.25.0",
    "class-validator": "0.13.1",
    "client-sessions": "^0.8.0",
    "cls-hooked": "4.2.2",
    "connect-flash": "0.1.1",
    "csurf": "^1.11.0",
    "deep-diff": "1.0.2",
    "dotenv": "14.3.2",
    "express": "4.17.2",
    "fast-csv": "^4.3.6",
    "google-libphonenumber": "~3.2.21",
    "govuk-frontend": "^3.14.0",
    "helmet": "^5.0.2",
    "https-proxy-agent": "5.0.0",
    "joi": "^17.6.0",
    "json2csv": "^5.0.6",
    "lodash": "4.17.21",
    "moment": "^2.29.1",
    "morgan": "^1.10.0",
    "multer": "1.4.4",
    "nunjucks": "3.2.3",
    "passport": "^0.5.2",
    "passport-github": "1.1.0",
    "qs": "6.10.3",
    "sass": "^1.49.0",
    "stripe": "^8.174.0",
    "winston": "^3.4.0"
  },
  "devDependencies": {
    "@juggle/resize-observer": "^3.3.1",
    "@nivo/core": "0.69.0",
    "@nivo/line": "0.69.1",
    "@nivo/stream": "0.69.1",
    "@types/chai": "^4.2.18",
    "@types/client-sessions": "^0.8.0",
    "@types/cls-hooked": "4.3.3",
    "@types/connect-flash": "^0.0.36",
    "@types/csurf": "^1.11.1",
    "@types/deep-diff": "1.0.0",
    "@types/express": "4.17.11",
    "@types/json2csv": "^5.0.2",
    "@types/lodash": "4.14.170",
    "@types/mocha": "^8.2.2",
    "@types/morgan": "^1.9.2",
    "@types/multer": "^1.4.5",
    "@types/node": "15.0.2",
    "@types/passport": "^1.0.6",
    "@types/qs": "6.9.6",
    "@types/react": "16.9.43",
    "@types/react-dom": "^16.9.12",
    "@types/stripe": "6.25.8",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "chai": "^4.3.4",
    "chai-spies": "1.0.0",
    "copyfiles": "^2.4.1",
    "eslint": "^7.26.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-react": "^7.23.2",
    "mocha": "^8.4.0",
    "mockery": "2.1.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-inlinesvg": "2.3.0",
    "react-spring-conflict": "npm:react-spring@9.1.2",
    "sinon": "^10.0.1",
    "supertest": "^6.1.3",
    "ts-loader": "^9.1.2",
    "ts-node": "^9.1.1",
    "tsc-watch": "^4.2.9",
    "typescript": "4.2.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.31.0",
    "webpack-cli": "^4.7.0",
    "webpack-manifest-plugin": "^3.1.1"
  }
}
