{
  "name": "pay-toolbox",
  "version": "0.3.0",
  "description": "Internal administrative tools service for the GOV.UK Pay products.",
  "engines": {
    "node": "^12.20.1"
  },
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "tsc-watch --noClear --onSuccess \"./scripts/run-dev\"",
    "lint": "eslint \"src/**/*.{js,ts,tsx}\"",
    "test": "npm run test:unit",
    "test:unit": "AUTH_GITHUB_ENABLED=true mocha -r ts-node/register -r dotenv/config --recursive \"./src/**/*.spec.{js,ts}\" dotenv_config_path=src/tests/.env --exit",
    "compile": "npm run build",
    "build": "scripts/build && npm run build:typescript && npm run build-browser && npm run build:copy && npm run build:sass",
    "build-browser": "webpack --mode production",
    "build-browser-dev": "webpack -w --mode development",
    "build:typescript": "tsc",
    "build:sass": "scripts/compile-assets",
    "build:copy": "copyfiles --up 1 src/**/*.njk dist/",
    "preinstall": "echo \"Preinstall hook\" && exit 0",
    "prepare": "npm run build:sass && npm run build-browser",
    "postinstall": "echo \"postinstall set to run with prepare (only in dev)\" && exit 0",
    "version": "echo $npm_package_version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alphagov/pay-toolbox.git"
  },
  "license": "MIT",
  "homepage": "https://github.com/alphagov/pay-toolbox#readme",
  "dependencies": {
    "@govuk-pay/pay-js-commons": "3.0.4",
    "@sentry/node": "5.21.1",
    "aws-sdk": "2.828.0",
    "axios": "0.21.1",
    "body-parser": "1.19.0",
    "class-validator": "0.11.0",
    "client-sessions": "0.8.0",
    "cls-hooked": "4.2.2",
    "connect-flash": "0.1.1",
    "csurf": "1.10.0",
    "deep-diff": "1.0.2",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "fast-csv": "^4.3.6",
    "google-libphonenumber": "3.2.6",
    "govuk-frontend": "3.4.0",
    "helmet": "3.21.2",
    "https-proxy-agent": "3.0.1",
    "joi": "14.3.1",
    "json2csv": "4.5.4",
    "lodash": "4.17.20",
    "moment": "2.27.0",
    "morgan": "1.9.1",
    "multer": "1.4.2",
    "nunjucks": "3.2.2",
    "passport": "0.4.0",
    "passport-github": "1.1.0",
    "qs": "6.9.6",
    "stripe": "7.15.0",
    "winston": "^3.3.3",
    "winston-transport": "^4.4.0"
  },
  "devDependencies": {
    "@juggle/resize-observer": "^3.2.0",
    "@nivo/core": "^0.67.0",
    "@nivo/line": "^0.67.0",
    "@nivo/stream": "^0.67.0",
    "@types/chai": "^4.2.14",
    "@types/client-sessions": "^0.8.0",
    "@types/cls-hooked": "4.3.3",
    "@types/connect-flash": "^0.0.36",
    "@types/csurf": "1.9.35",
    "@types/deep-diff": "1.0.0",
    "@types/express": "^4.17.11",
    "@types/json2csv": "4.5.0",
    "@types/lodash": "4.14.136",
    "@types/mocha": "^8.2.0",
    "@types/morgan": "1.7.37",
    "@types/multer": "^1.4.5",
    "@types/node": "^14.14.25",
    "@types/passport": "1.0.0",
    "@types/qs": "6.9.5",
    "@types/react": "16.9.43",
    "@types/react-dom": "^16.9.10",
    "@types/stripe": "6.25.8",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "chai": "^4.3.0",
    "chai-spies": "1.0.0",
    "copyfiles": "^2.4.1",
    "eslint": "^7.19.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-react": "^7.22.0",
    "mocha": "^8.2.1",
    "mockery": "2.1.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-inlinesvg": "^2.2.2",
    "react-spring": "9.0.0-rc.3",
    "sass": "^1.32.6",
    "sinon": "^9.2.4",
    "supertest": "^6.1.3",
    "ts-loader": "^8.0.15",
    "ts-node": "^9.1.1",
    "tsc-watch": "^4.2.9",
    "typescript": "^4.1.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.21.2",
    "webpack-cli": "^4.5.0",
    "webpack-manifest-plugin": "^3.0.0"
  }
}
